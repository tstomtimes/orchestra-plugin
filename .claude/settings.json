{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(git:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(curl:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(echo:*)",
      "Bash(mkdir:*)",
      "Bash(chmod:*)",
      "Bash(python3:*)",
      "Bash(node:*)",
      "Bash(jq:*)",
      "Bash(rm:*)",
      "Bash(pkill:*)",
      "Bash(lsof:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(if:*)",
      "Bash(for:*)",
      "Bash(bash:*)",
      "Bash(sleep:*)",
      "Bash(tree:*)",
      "Bash(pwd:*)",
      "Bash(cd:*)",
      "Bash(./setup.sh:*)",
      "Bash(./hooks/*.sh:*)",
      "Bash(./mcp-servers/*.sh:*)",
      "Read(.*)",
      "Write(.*)",
      "Edit(.*)",
      "Glob(.*)",
      "Grep(.*)",
      "Task(.*)",
      "TodoWrite(.*)",
      "WebFetch(domain:*)",
      "SlashCommand(.*)",
      "NotebookEdit(.*)",
      "BashOutput(.*)",
      "KillShell(.*)",
      "ExitPlanMode(.*)"
    ],
    "deny": [],
    "ask": []
  },
  "enabledPlugins": {
    "orchestra@orchestra-marketplace": true
  },
  "extraKnownMarketplaces": {
    "orchestra-marketplace": {
      "source": {
        "source": "directory",
        "path": "."
      }
    }
  }
}
